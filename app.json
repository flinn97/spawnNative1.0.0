{
  "expo": {
    "name": "Spawn",
    "slug": "Spawnrpg",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SpawnLogoIcon.png",
    "backgroundColor": "#A80303",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spawnLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#EF3E23",
      "padding": "18%"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "ios": {
      "bundleIdentifier": "com.spawnrpg.spawn",
      "supportsTablet": true,
      "useFrameworks": "static",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
     }
    },
    "android": {
      "package": "com.ava.spawnrpg",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/SpawnLogoIcon.png",
        "backgroundColor": "#EF3E23"
      }
    },
    "plugins": [
      "@react-native-firebase/app"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1e8af536-05f5-4f50-8e99-56896d55fdab"
      }
    }
  }
}
